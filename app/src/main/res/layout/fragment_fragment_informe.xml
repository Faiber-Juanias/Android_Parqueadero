<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    tools:context=".FragmentInforme">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:id="@+id/btn_entradas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="12dp"
        android:text="@string/btn_entradas"/>

    <Button
        android:id="@+id/btn_salidas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="29dp"
        android:text="@string/btn_salidas"/>

    <ListView
        android:id="@+id/lista_informe"
        android:layout_width="match_parent"
        android:layout_height="440dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"></ListView>

</RelativeLayout>